{
  "timeout": 10,
  "source": {
    "directories": [
      "src"
    ]
  },
  "phpUnit": {
    "configDir": "tests\/"
  },
  "logs": {
    "text": "tests/test-results/infection.log",
    "summary": "tests/test-results/summary.log",
    "perMutator": "tests/test-results/per-mutator.md",
    "badge": {
      "branch": "master"
    }
  },
  "mutators": {
    "@default": true,
    "IdenticalEqual": false,
    "NotIdenticalNotEqual": false
  }
}
